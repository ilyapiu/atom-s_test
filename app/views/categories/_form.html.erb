<%= form_with model: @category do |f|%>
   <div class="mb-4 mt-4">
    <%= f.label :photo%>
    <%= f.file_field (:photo), placeholder:'Фото', class:'form-control' %>
  </div>

  <div class="mb-4 mt-4">
    <%= f.label :name%>
    <%= f.text_field (:name), placeholder:'Название', class:'form-control' %>
  </div>

  <%= f.submit "Добавить категорию", class:'btn btn-primary'%>
<%end%>